{
  "name": "@resonance/runtime",
  "version": "0.1.0",
  "description": "Resonance: Tail-tolerant distributed runtime with synchronization intelligence",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:agent-binaries": "node scripts/build-agent-binaries.js",
    "test": "jest --coverage",
    "bench": "bash bench/run.sh",
    "report": "bash bench/report.sh",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "resonance",
    "latency",
    "tail-latency",
    "kuramoto",
    "synchronization",
    "performance"
  ],
  "author": "Christopher Stringer",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-metrics": "^1.8.0",
    "hdr-histogram-js": "^3.0.0",
    "adm-zip": "^0.5.10"
  },
  "devDependencies": {
    "archiver": "^5.3.2",
    "@types/adm-zip": "^0.4.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "pkg": {
    "assets": [
      "policy/**/*"
    ],
    "targets": [
      "node18-macos-x64",
      "node18-linux-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist-binaries"
  }
}
